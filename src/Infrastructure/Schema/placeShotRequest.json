{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "letter": {
      "type": "string",
      "pattern": "^[A-J]"
    },
    "number": {
      "type": "integer",
      "minimum": 1,
      "maximum": 10
    }
  },
  "additionalProperties": false,
  "required": [
    "letter",
    "number"
  ]
}
